{
  "breadcrumbs.enabled": true,
  "chat.commandCenter.enabled": false,
  "continue.enableTabAutocomplete": false,
  "continue.disableQuickFix": true,
  "continue.telemetryEnabled": false,
  "debug.console.fontSize": 14,
  "debug.internalConsoleOptions": "neverOpen",
  "debug.javascript.autoAttachFilter": "disabled",
  "editor.acceptSuggestionOnEnter": "on",
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "always",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.defaultIndentationSize": 4,
  "editor.folding": false,
  "editor.fontFamily": "'Fira Code Retina', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 16,
  "editor.fontWeight": "normal",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "editor.glyphMargin": false,
  "editor.lineNumbers": "off",
  "editor.minimap.enabled": false,
  "editor.parameterHints.enabled": true,
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "on"
  },
  "editor.snippetSuggestions": "top",
  "editor.stickyScroll.enabled": true,
  "editor.suggest.matchOnWordStartOnly": true,
  "editor.suggest.preview": true,
  "editor.suggest.showMethods": true,
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "emmet.showSuggestionsAsSnippets": false,
  "emmet.triggerExpansionOnTab": false,
  "git.autofetch": true,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.plusFeatures.enabled": false,
  "python.analysis.completeFunctionParens": true,
  "python.experiments.enabled": false,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.terminal.activateEnvironment": false,
  "python.terminal.activateEnvInCurrentTerminal": false,
  "python.terminal.executeInFileDir": true,
  "redhat.telemetry.enabled": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.fontFamily": "'Fira Code Retina', Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 16,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.smartRelativeLine": true,
  "vim.sneak": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.useSystemClipboard": true,
  "window.zoomLevel": 2,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "[docker]": {
    "editor.defaultFormatter": null
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[shell]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[shellscript]": {
    "editor.defaultFormatter": null
  },
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "server": "localhost",
      "port": 5432,
      "driver": "PostgreSQL",
      "name": "pagila",
      "database": "pagila",
      "username": "postgres",
      "password": "not_a_secret_password"
    },
    {
      "mysqlOptions": {
        "authProtocol": "xprotocol"
      },
      "previewLimit": 50,
      "server": "localhost",
      "port": 33060,
      "driver": "MySQL",
      "name": "sakila",
      "database": "sakila",
      "username": "root",
      "password": "not_a_secret_password",
      "connectionTimeout": 30
    }
  ],
  "workbench.activityBar.location": "hidden"
}
